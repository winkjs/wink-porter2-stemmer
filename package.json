  "bugs": {
    "url": "https://github.com/decisively/wink-porter2-stemmer/issues"
  },
  "homepage": "https://github.com/decisively/wink-porter2-stemmer#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "docco": "^0.7.0",
    "eslint": "^3.13.1",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.4",
    "mocha": "^3.1.0",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
